            "with"
          ],
          "type": "custom",
          "values": [
            "family",
            "friends",
            "parents",
            "children",
            "wife",
            "spouse"
          ],
          "default": "not specified",
          "prompt": "Who did you spend time with?"
        },
        "date": {
          "type": "AMAZON.DATE",
          "default": "TODAY",
          "prompt": "When did you do this?"
        },
        "confirmation": "Thanks. I will record that {date} you spent {qty} minutes eating with family or friends. Ok?",
        "cancel": "Sorry, please try again.",
        "dailyTarget": "45",
        "weeklyTarget": "0",
        "monthlyTarget": "0"
      },
